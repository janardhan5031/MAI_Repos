    "express": "^4.18.2",
    "fast-csv": "^5.0.1",
    "ffmpeg": "^0.0.4",
    "fluent-ffmpeg": "^2.1.2",
    "graphql": "^16.5.0",
    "graphql-type-json": "^0.3.2",
    "jaas": "^1.0.0",
    "kafkajs": "^2.2.4",
    "migrate-mongo": "^11.0.0",
    "moment": "^2.29.3",
    "moment-timezone": "^0.5.43",
    "mongoose": "^8.2.1",
    "node-cron": "^3.0.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sharp": "^0.32.6",
