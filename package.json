    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/throttler": "^2.0.1",
    "apollo-server-express": "^3.8.1",
    "aws-sdk": "^2.1510.0",
    "axios": "^1.7.2",
    "cache-manager": "^4.0.1",
    "cache-manager-redis-store": "^2.0.0",
